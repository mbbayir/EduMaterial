<!DOCTYPE html>
<html lang="en" class="sidebar-left-big-icons">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EduMaterial</title>

    <!-- Web Fonts  -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="/Panel/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="/Panel/vendor/animate/animate.compat.css">
    <link rel="stylesheet" href="/Panel/vendor/font-awesome/css/all.min.css" />
    <link rel="stylesheet" href="/Panel/vendor/boxicons/css/boxicons.min.css" />
    <link rel="stylesheet" href="/Panel/vendor/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" href="/Panel/vendor/bootstrap-datepicker/css/bootstrap-datepicker3.css" />
    <link rel="stylesheet" href="/Panel/vendor/jquery-ui/jquery-ui.css" />
    <link rel="stylesheet" href="/Panel/vendor/jquery-ui/jquery-ui.theme.css" />
    <link rel="stylesheet" href="/Panel/vendor/bootstrap-multiselect/css/bootstrap-multiselect.css" />
    <link rel="stylesheet" href="/Panel/vendor/morris/morris.css" />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="/Panel/css/theme.css" />

    <!-- Skin CSS -->
    <link rel="stylesheet" href="/Panel/css/skins/default.css" />

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="/Panel/css/custom.css">

    <!-- Head Libs -->
    <script src="/Panel/vendor/modernizr/modernizr.js"></script>


    @await RenderSectionAsync("Styles", required: false)

</head>
<body>
    <section class="body">

        <!-- start: header -->
        @await Html.PartialAsync("/Views/Shared/Partials/_HeaderPartialView.cshtml")
        <!-- end: header -->

        <div class="inner-wrapper">
            <!-- start: sidebar -->
            <aside id="sidebar-left" class="sidebar-left">

                <div class="sidebar-header">
                    <div class="sidebar-title">
                        Navigation
                    </div>
                    <div class="sidebar-toggle d-none d-md-block" data-toggle-class="sidebar-left-collapsed" data-target="html" data-fire-event="sidebar-left-toggle">
                        <i class="fas fa-bars" aria-label="Toggle sidebar"></i>
                    </div>
                </div>

                <div class="nano">
                    <div class="nano-content">
                        <nav id="menu" class="nav-main" role="navigation">
                            @await Html.PartialAsync("/Views/Shared/Partials/_SidebarPartialView.cshtml")
                        </nav>

                    </div>

                </div>

            </aside>
            <!-- end: sidebar -->

            <section role="main" class="content-body">
                @RenderBody()

            </section>
        </div>


    </section>
    <!-- Vendor -->
    <script src="/Panel/vendor/jquery/jquery.js"></script>
    <script src="/Panel/vendor/jquery-browser-mobile/jquery.browser.mobile.js"></script>
    <script src="/Panel/vendor/popper/umd/popper.min.js"></script>
    <script src="/Panel/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/Panel/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="/Panel/vendor/common/common.js"></script>
    <script src="/Panel/vendor/nanoscroller/nanoscroller.js"></script>
    <script src="/Panel/vendor/magnific-popup/jquery.magnific-popup.js"></script>
    <script src="/Panel/vendor/jquery-placeholder/jquery.placeholder.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/izitoast/dist/js/iziToast.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
    <!-- Theme Base, Components and Settings -->
    <script src="/Panel/js/theme.js"></script>

    <!-- Theme Custom -->
    <script src="/Panel/js/custom.js"></script>

    <!-- Theme Initialization Files -->
    <script src="/Panel/js/theme.init.js"></script>

    <!-- Examples -->
    <script src="/Panel/js/examples/examples.dashboard.js"></script>

</body>
</html>
